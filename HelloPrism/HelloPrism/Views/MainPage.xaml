             xmlns:local="clr-namespace:HelloPrism"
             x:Class="HelloPrism.Views.MainPage">
    <StackLayout>
        <Label Text="{Binding WelcomeMessage}"
               TextColor="{StaticResource BlackText}"
               FontSize="18"
               Margin="0,20"
               HorizontalOptions="Center" />
    </StackLayout>


</ContentPage>
